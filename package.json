    "bip32grs": "^2.0.5",
    "bip39": "^3.0.2",
    "bitcoinjs-message": "^2.2.0",
    "blake-hash": "^2.0.0",
    "bn.js": "^5.1.2",
    "bs58": "^4.0.1",
    "bs58grscheck": "^2.1.2",
    "bs58smartcheck": "^2.0.4",
    "cleaners": "^0.3.17",
    "disklet": "^0.4.5",
    "ecpair": "https://github.com/EdgeApp/ecpair.git#b193eb8ea2ec0c93b528f4b0223a605407ff43e4",
    "edge-sync-client": "^0.2.7",
    "memlet": "^0.1.7",
    "uri-js": "^4.4.0",
    "url-parse": "^1.5.2",
    "wif-smart": "^2.0.0",
    "wifgrs": "^2.0.6",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "@sucrase/webpack-loader": "^2.0.0",
