    "node-fetch": "2.x",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "parcel": "^1.12.4",
    "patch-package": "^8.0.0",
    "prettier": "^2.2.0",
